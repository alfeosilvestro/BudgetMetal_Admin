<script>
    $(function () {
       
        
        $('#ValidRfqdate').bootstrapMaterialDatePicker('setMinDate', moment());
        $('#StartRfqdate').bootstrapMaterialDatePicker('setMinDate', moment()).on('change', function (e, date) {
            $('#ValidRfqdate').bootstrapMaterialDatePicker('setMinDate', date);
        });

        $('#EstimatedProjectEndDate').bootstrapMaterialDatePicker('setMinDate', moment());
        $('#EstimatedProjectStartDate').bootstrapMaterialDatePicker('setMinDate', moment()).on('change', function (e, date) {
            $('#EstimatedProjectEndDate').bootstrapMaterialDatePicker('setMinDate', date);
        });


    });



 

    $("#btnDraft").click(function () {
        SaveForm();
    });

    function SaveForm() {
        //alert("Start save!")
        $("#infoTag").click();
        var isValid = true;

        //$("form#frmCreateRFQ :input[type=text]").each(function () {
        //    var input = $(this);
        //    if (input.val() == "") {
        //        isValid = false;
        //    }
        //});

        var invalidMessage = "";

        isValid = $("form#frmCreateRFQ").valid();
        if (isValid) {
            //check Invited Supplier
            

            var hasStartRfqDate = false;
            var startRfqDate = $('#StartRfqDate').val();
            if (startRfqDate != "") {
                hasStartRfqDate = true;
            } else {
                invalidMessage += "Start RFQ date should not be blank. \n";
            }

            var hasValidRfqDate = false;
            var validRfqDate = $('#ValidRfqDate').val();
            if (validRfqDate != "") {
                hasValidRfqDate = true;
            } else {
                invalidMessage += "Valid RFQ date should not be blank. \n";
            }

            var hasEstimateProjectStartDate = false;
            var estimateProjectStartDate = $('#EstimateProjectStartDate').val();
            if (estimateProjectStartDate != "") {
                hasEstimateProjectStartDate = true;
            } else {
                invalidMessage += "Estimate project start date should not be blank. \n";
            }

            var hasEstimateProjectEndDate = false;
            var estimateProjectEndDate = $('#EstimateProjectEndDate').val();
            if (estimateProjectEndDate != "") {
                hasEstimateProjectEndDate = true;
            } else {
                invalidMessage += "Estimate project end date should not be blank. \n";
            }

            var hasQuotationOpeningDate = false;
            var quotationOpeningDate = $('#QuotationOpeningDate').val();
            if (quotationOpeningDate != "") {
                hasQuotationOpeningDate = true;
            } else {
                invalidMessage += "Quotation opening date should not be blank. \n";
            }

            var hasContractValue = false;
            var contractValue = $('#ContractValue').val();
            if (contractValue != "") {
                hasContractValue = true;
            } else {
                invalidMessage += "Contract value should not be blank. \n";
            }

            //Check Requeirment Table
            var hasRequirementRecord = false;
            var hasRequirementValid = false;
            var requirementTableCount = $('#requirementsTableBody').find('tr').length;
            if (requirementTableCount > 0) {
                hasRequirementRecord = true;
            }

            if (hasRequirementRecord) {
                $('input:text.requirement').each(function () {
                    //hasRequirementValid = false;
                    if ($(this).val() == "") {
                        hasRequirementValid = true;
                    }
                });

                if (hasRequirementValid) {
                    invalidMessage += "Service name in Requirement tab is not blank. \n";
                }
            } else {
                invalidMessage += "Please fill at least one record in Requirement tab. \n";
            }

            //Check Support Table
            var hasSupportRecord = false;
            var hasSupportValid = false;
            var supportTableCount = $('#slaTableBody').find('tr').length;
            if (supportTableCount > 0) {
                hasSupportRecord = true;
            }
            if (hasSupportRecord) {
                $('input:text.support').each(function () {
                    //hasSupportValid = false;
                    if ($(this).val() == "") {
                        hasSupportValid = true;
                    }

                    if (hasSupportValid) {
                        invalidMessage += "Service name in Support tab is not blank. \n";
                    }
                });
            } else {
                invalidMessage += "Please fill at least one record in Support tab. \n";
            }

            //Check Commercial Table
            var hasCommercialRecord = false;
            var hasCommercialValid = false;
            var commercialTableCount = $('#penaltyTableBody').find('tr').length;
            if (commercialTableCount > 0) {
                hasCommercialRecord = true;
            }
            if (hasCommercialRecord) {
                $('input:text.commercial').each(function () {
                    //hasCommercialValid = false;
                    if ($(this).val() == "") {
                        hasCommercialValid = true;
                    }
                });

                if (hasCommercialValid) {
                    invalidMessage += "Item name in Commercial tab is not blank. \n";
                }
            } else {
                invalidMessage += "Please fill at least one record in Commercial tab. \n";
            }

            //Check Pricing Table
            var hasPricingRecord = false;
            var hasPricingValid = false;
            var pricingTableCount = $('#pricingTableBody').find('tr').length;
            if (pricingTableCount > 0) {
                hasPricingRecord = true;
            }
            if (hasPricingRecord) {
                $('input:text.pricing').each(function () {
                    //hasPricingValid = false;
                    if ($(this).val() == "") {
                        hasPricingValid = true;
                    }
                });

                if (hasPricingValid) {
                    invalidMessage += "Pricing name in Pricing tab is not blank. \n";
                }
            } else {
                invalidMessage += "Please fill at least one record in Product Pricing tab. \n";
            }

            //Service Table
            var hasServicePricingRecord = false;
            var hasServicePricingValid = false;
            var pricingServiceTableCount = $('#pricingServiceTableBody').find('tr').length;
            if (pricingServiceTableCount > 0) {
                hasServicePricingRecord = true;
            }
            if (hasServicePricingRecord) {
                $('input:text.pricing').each(function () {
                    //hasServicePricingValid = false;
                    if ($(this).val() == "") {
                        hasServicePricingValid = true;
                    }
                });

                if (hasServicePricingValid) {
                    invalidMessage += "Pricing name in Service Pricing tab is not blank. \n";
                }
            } else {
                invalidMessage += "Please fill at least one record in Service Product Pricing tab. \n";
            }

            //Warranty Table
            var hasWarrantyPricingRecord = false;
            var hasWarrantyPricingValid = false;
            var pricingWarrantyTableCount = $('#pricingWarrantyTableBody').find('tr').length;
            if (pricingWarrantyTableCount > 0) {
                hasWarrantyPricingRecord = true;
            }
            if (hasWarrantyPricingRecord) {
                $('input:text.pricing').each(function () {
                    //hasWarrantyPricingValid = false;
                    if ($(this).val() == "") {
                        hasWarrantyPricingValid = true;
                    }
                });

                if (hasWarrantyPricingValid) {
                    invalidMessage += "Pricing name in Warranty Pricing tab is not blank. \n";
                }
            } else {
                invalidMessage += "Please fill at least one record in Warranty Pricing tab. \n";
            }

            //Check PricingValue Table
            var hasPricingQtyValid = false;
            var hasPricingQtyBlank = false;
            $('input:text.pricingQty').each(function () {
                //hasPricingQtyBlank = false;
                var qty = $(this).val();
                //hasPricingQtyBlank = false;
                if (qty != "") {
                    //hasPricingQtyValid = false;
                    if (isNaN(qty)) {
                        hasPricingQtyValid = true;
                    } 
                } else {
                    hasPricingQtyBlank = true;
                }
            });
            if (hasPricingQtyBlank) {
                invalidMessage += "Quantity in Pricing tab is not blank. \n";
            }
            if (hasPricingQtyValid) {
                invalidMessage += "Quantity in Pricing tab is not number. \n";
            }
            
            //if (hasStartRfqDate && hasValidRfqDate && hasEstimateProjectStartDate && haveInvitedSupplier && hasRequirementValid && hasSupportValid && hasCommercialValid && hasPricingValid && hasPricingQtyValid && !hasPricingQtyBlank && hasRequirementRecord && hasSupportRecord && hasCommercialRecord && hasPricingRecord && hasServicePricingRecord && hasWarrantyPricingRecord) {
            if (invalidMessage == "") {
                $('.page-loader-wrapper').fadeIn();
                $("#frmCreateRFQ").submit();

            } else {
                alert(invalidMessage);
                invalidMessage = "";
            }
        } else {
            alert("Please fill all fields!")
        }
    }


    $('#btnUploadFile').on('click', function () {
