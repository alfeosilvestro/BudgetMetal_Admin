                        <li role="presentation"><a href="#detailed_requirements" data-toggle="tab">Requirements</a></li>
                        <li role="presentation"><a href="#service_level_agreement" data-toggle="tab">Supports</a></li>
                        <li role="presentation"><a href="#penalty" data-toggle="tab">Commercials</a></li>
