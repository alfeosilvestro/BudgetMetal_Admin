@model Com.BudgetMetal.ViewModels.Rfq.VmRfqItem
@{
    ViewData["Title"] = "Create";
}
<form asp-controller="Rfq" asp-action="Create" method="post" enctype="multipart/form-data" id="frmCreateRFQ">
    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
    <div class="block-header">
        <h2>Prepare Request for Quotation</h2>
    </div>
    <!-- Input -->
    <div class="row clearfix">
        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
            <div class="card">
                <div class="header">
                    <h2>
                        General Information
                    </h2>
                </div>
                <div class="body">

                    <div class="row clearfix">
                        <div class="col-sm-4">
                            <label for="quote_no">RFQ Reference</label>
                            <div class="form-group">
                                <div class="form-line">
                                    <input type="text" asp-for="Document.DocumentNo" readonly class="form-control" placeholder="<Auto Generate by System>">
                                    <input type="hidden" asp-for="Document.DocumentStatus_Id" value="@(Com.BudgetMetal.Common.Constants_CodeTable.Code_RFQ_Draft)">
                                    <input type="hidden" asp-for="Document.DocumentType_Id" value="@(Com.BudgetMetal.Common.Constants_CodeTable.Code_RFQ)">
                                    <input type="hidden" asp-for="Document.Company_Id" value="@ViewBag.Company_Id">
                                    <input type="hidden" asp-for="Document.CreatedBy" value="@ViewBag.UserName">
                                    <input type="hidden" asp-for="Document.UpdatedBy" value="@ViewBag.UserName">
                                    <input type="hidden" asp-for="CreatedBy" value="@ViewBag.UserName">
                                    <input type="hidden" asp-for="UpdatedBy" value="@ViewBag.UserName">
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-6">
                            <label for="quote_no">Title</label>
                            <div class="form-group">
                                <div class="form-line">
                                    <input type="text" asp-for="Document.Title" class="form-control" placeholder="Project Title" />
                                    <span asp-validation-for="Document.Title" class="text-danger"></span>
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-2">
                            <label for="status">Status</label>
                            <h4><span class="label label-info">New</span></h4>
                        </div>
                        @*<div class="col-sm-6">
                                <label for="quote_no">Internal Reference No</label>
                                <div class="form-group">
                                    <div class="form-line">
                                        <input type="text" asp-for="InternalRefrenceNo" class="form-control" placeholder="Internal Reference">
                                        <span asp-validation-for="InternalRefrenceNo" class="text-danger"></span>
                                    </div>
                                </div>
                            </div>*@
                        @*<div class="col-sm-6">
                                <label for="quote_no">Internal Project Name</label>
                                <div class="form-group">
                                    <div class="form-line">
                                        <input type="text" asp-for="InternalProjectName" class="form-control" placeholder="Internal Project Name">
                                        <span asp-validation-for="InternalProjectName" class="text-danger"></span>
                                    </div>
                                </div>
                            </div>*@

                    </div>

                </div>
            </div>
        </div>
    </div>
    <!-- #END# Input -->
    <!-- Tabs -->
    <div class="row">
        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
            <div class="card">
                <div class="body">
                    <!-- Nav tabs -->
                    <ul class="nav nav-tabs tab-nav-right" role="tablist">
                        <li role="presentation" class="active"><a id="infoTag" href="#basic_requirements" data-toggle="tab">Info</a></li>
                        <li role="presentation"><a href="#detailed_requirements" data-toggle="tab">Products</a></li>
                        <li role="presentation"><a href="#service_level_agreement" data-toggle="tab">Supports</a></li>
                        <li role="presentation"><a href="#penalty" data-toggle="tab">Commercials</a></li>
                        <li role="presentation"><a href="#pricing" data-toggle="tab">Pricing</a></li>
                        <li role="presentation"><a href="#attachments" data-toggle="tab">Attachments</a></li>
                        <li role="presentation"><a href="#settings" data-toggle="tab">Roles</a></li>
                        <li role="presentation"><a href="#invited_suppliers" data-toggle="tab">Invite Supplier</a></li>
                    </ul>
                    <!-- Tab panes -->
                    <div class="tab-content">
                        <div role="tabpanel" class="tab-pane fade in active" id="basic_requirements">
                            
                            <div class="row clearfix">
                                <div class="col-sm-3">
                                    <label for="StartRfqdate">RFQ Start Date <a href="#" data-toggle="modal" data-target="#timelineModal"><span class="badge bg-light-blue">i</span></a></label>
                                    <div class="form-group">
                                        <div class="form-line">
                                            <input type="text" asp-for="StartRfqdate" class="form-control datepicker" placeholder="Start RFQ Date">
                                            <span asp-validation-for="StartRfqdate" class="text-danger"></span>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-sm-3">
                                    <label for="ValidRfqdate">Valid RFQ Date <a href="#" data-toggle="modal" data-target="#timelineModal"><span class="badge bg-light-blue">i</span></a></label>
                                    <div class="form-group">
                                        <div class="form-line">
                                            <input type="text" asp-for="ValidRfqdate" class="form-control datepicker" placeholder="End RFQ Date">
                                            <span asp-validation-for="ValidRfqdate" class="text-danger"></span>
                                        </div>
                                    </div>
                                </div>

                                <div class="col-sm-3">
                                    <label for="QuotationOpeningDate">Quotation Opening Date <a href="#" data-toggle="modal" data-target="#timelineModal"><span class="badge bg-light-blue">i</span></a></label>
                                    <div class="form-group">
                                        <div class="form-line">
                                            <input type="text" asp-for="QuotationOpeningDate" class="form-control datepicker" placeholder="Quotation Opening Date">
                                            <span asp-validation-for="QuotationOpeningDate" class="text-danger"></span>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="row clearfix">
                                <div class="col-sm-3">
                                    <label for="quote_no">Project Start Date <a href="#" data-toggle="modal" data-target="#timelineModal"><span class="badge bg-light-blue">i</span></a></label>
                                    <div class="form-group">
                                        <div class="form-line">
                                            <input type="text" asp-for="EstimatedProjectStartDate" class="form-control datepicker" placeholder="Start of Project Delivery">
                                            <span asp-validation-for="EstimatedProjectStartDate" class="text-danger"></span>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-sm-3">
                                    <label for="EstimatedProjectEndDate">Project End Date <a href="#" data-toggle="modal" data-target="#timelineModal"><span class="badge bg-light-blue">i</span></a></label>
                                    <div class="form-group">
                                        <div class="form-line">
                                            <input type="text" asp-for="EstimatedProjectEndDate" class="form-control datepicker" placeholder="End of Project Delivery">
                                            <span asp-validation-for="EstimatedProjectEndDate" class="text-danger"></span>
                                        </div>
                                    </div>
                                </div>
                                
                            </div>
                            <div class="row clearfix">
                                <div class="col-sm-9">
                                    <label for="quote_no">Contract Value</label>
                                    <div class="form-group">
                                        <div class="form-line">
                                            <input type="text" asp-for="ContractValue" class="form-control" placeholder="Contract Value">
                                            <span asp-validation-for="ContractValue" class="text-danger"></span>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="row clearfix">

                                <div class="col-sm-3">
                                    <input type="checkbox" asp-for="IsPublic"  class="chk-col-red" />
                                    <label for="IsPublic">Public </label>
                                    <a href="#" data-toggle="modal" data-target="#publicModal"><span class="badge bg-light-blue">i</span></a>
                                </div>
                                <div class="col-sm-3">
                                    <input type="checkbox" asp-for="SupplierProvideInstallationService" class="chk-col-red" />
                                    <label for="SupplierProvideInstallationService">Supplier Provide Installation Service</label>
                                </div>
                                <div class="col-sm-6">
                                    &nbsp;
                                </div>

                            </div>
                            <div class="row clearfix">
                                <div class="col-sm-12">
                                    <label for="quote_no">Messages to Suppliers</label>
                                    <div class="form-group">
                                        <div class="form-line">
                                            <textarea rows="3" asp-for="MessageToSupplier" class="form-control no-resize" placeholder="Messages to Suppliers"></textarea>
                                            <span asp-validation-for="MessageToSupplier" class="text-danger"></span>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="row clearfix">
                                <div class="col-sm-6">
                                    <span class="text-warning">Click <a href='@Url.Action("DownloadExcelTemplate", "Rfq")'>Here</a> to Download Template</span>
                                    <a href="#" data-toggle="modal" data-target="#excelInfoModal"><span class="badge bg-light-blue">i</span></a><br /><br />
                                    <label>Upload Template</label>
                                    <input type="file" name="fUpload" id="fUpload" value="" class="form-control" />
                                    <br />
                                    <button type="button" class="btn btn-success" id="btnUploadFile">Upload Template</button>
                                </div>
                            </div>
                        </div>

                        <div role="tabpanel" class="tab-pane fade in" id="detailed_requirements">
                            <input type="hidden" id="detailRequirementLastId" value="1" />
                            <table id="requirementsTable" class="table table-striped">
                                <thead>
                                    <tr>
                                        <th width="5%">Serial</th>
                                        <th width="40%">Service Name</th>
                                        <th width="50%">Description</th>
                                        <th>&nbsp;</th>
                                    </tr>
                                </thead>
                                <tbody id="requirementsTableBody">
                                    <tr>
                                        <td>1</td>
                                        <td><input type="text" asp-for="Requirement[0].ServiceName" class="form-control requirement" /></td>
                                        <td><textarea rows="3" asp-for="Requirement[0].Description" class="form-control"></textarea></td>
                                        <td><button type="button" class="btn btn-danger waves-effect" onclick='RemoveRequirementRow(this)'>Remove</button></td>
                                    </tr>
                                </tbody>
                            </table>
                            <div class="row">
                                <div class="form-group col-sm-12">
                                    <a class="btn btn-success" id="btnNewRequirement"> New Requirement </a>
                                </div>

                            </div>
                        </div>
                        <div role="tabpanel" class="tab-pane fade in" id="service_level_agreement">
                            <input type="hidden" id="detailSLALastId" value="1" />
                            <table id="slaTable" class="table table-striped">
                                <thead>
                                    <tr>
                                        <th width="5%">Serial</th>
                                        <th width="40%">Service Name</th>
                                        <th width="50%">Description</th>
                                        <th>&nbsp;</th>
                                    </tr>
                                </thead>
                                <tbody id="slaTableBody">
                                    <tr>
                                        <td>1</td>
                                        <td><input type="text" asp-for="Sla[0].Requirement" class="form-control support" /></td>
                                        <td><textarea rows="3" asp-for="Sla[0].Description" class="form-control"></textarea></td>
                                        <td><button type="button" class="btn btn-danger waves-effect" onclick='RemoveSLARow(this)'>Remove</button></td>
                                    </tr>
                                </tbody>
                            </table>
                            <div class="row">
                                <div class="form-group col-sm-12">
                                    <a class="btn btn-success" id="btnNewSLA"> New Support </a>
                                </div>
                            </div>
                        </div>
                        <div role="tabpanel" class="tab-pane fade in" id="penalty">
                            <input type="hidden" id="PenaltyLastId" value="1" />
                            <table id="penaltyTable" class="table table-striped">
                                <thead>
                                    <tr>
                                        <th width="5%">Serial</th>
                                        <th width="40%">Service Name</th>
                                        @*<th width="20%">Milestone(s)</th>*@
                                        <th width="50%">Description</th>
                                        <th>&nbsp;</th>
                                    </tr>
                                </thead>
                                <tbody id="penaltyTableBody">
                                    <tr>
                                        <td>1</td>
                                        <td><input type="text" asp-for="Penalty[0].BreachOfServiceDefinition" class="form-control commercial" /></td>
                                        @*<td><input type="text" asp-for="Penalty[0].PenaltyAmount" class="form-control" /></td>*@
                                        <td><textarea rows="3" asp-for="Penalty[0].Description" class="form-control"></textarea></td>
                                        <td><button type="button" class="btn btn-danger waves-effect" onclick='RemovePenaltyRow(this)'>Remove</button></td>
                                    </tr>
                                </tbody>
                            </table>
                            <div class="row">
                                <div class="form-group col-sm-12">
                                    <a class="btn btn-success" id="btnNewPenalty">New Commercial </a>
                                </div>
                            </div>
                        </div>

                        <div role="tabpanel" class="tab-pane fade in" id="pricing">
                            <input type="hidden" id="PricingLastId" value="3" />
                            <table id="pricingTable" class="table table-striped">
                                <thead>
                                    <tr>
                                        <th width="5%">Serial</th>
                                        <th width="30%">Item Name</th>
                                        <th width="40%">Item Description</th>
                                        @*<th width="20%">Internal Reference Code</th>*@
                                        <th width="15%">Quantity</th>
                                        <th>&nbsp;</th>
                                    </tr>
                                </thead>
                                <tbody id="pricingTableBody">
                                    <tr>
                                        <td>1</td>
                                        <td><input type="text" asp-for="RfqPriceSchedule[0].ItemName" class="form-control pricing" /></td>
                                        @*<td><input type="text" asp-for="RfqPriceSchedule[0].ItemDescription" class="form-control" /></td>*@
                                        <td><textarea rows="3" asp-for="RfqPriceSchedule[0].ItemDescription" class="form-control"></textarea></td>
                                        @*<td><input type="text" asp-for="RfqPriceSchedule[0].InternalRefrenceCode" class="form-control" /></td>*@
                                        <td>
                                            <input type="text" asp-for="RfqPriceSchedule[0].QuantityRequired" class="form-control pricingQty" />
                                            <input type="hidden" asp-for="RfqPriceSchedule[0].CategoryId" value="@(Com.BudgetMetal.Common.Constants_CodeTable.Code_RfqPriceCategory_Product)" class="form-control" />
                                        </td>

                                        <td><button type="button" class="btn btn-danger waves-effect" onclick='RemovePricingRow(this)'>Remove</button></td>
                                    </tr>
                                </tbody>

                            </table>
                            <div class="row">
                                <div class="form-group col-sm-12">
                                    <a class="btn btn-success" id="btnNewPricing"> Add Product Pricing  </a>
                                </div>
                            </div>

                            @*<input type="hidden" id="PricingServiceLastId" value="1" />*@
                            <table id="pricingServiceTable" class="table table-striped">
                                <thead>
                                    <tr>
                                        <th width="5%">Serial</th>
                                        <th width="30%">Item Name</th>
                                        <th width="40%">Item Description</th>
                                        @*<th width="20%">Internal Reference Code</th>*@
                                        <th width="15%">Quantity</th>
                                        <th>&nbsp;</th>
                                    </tr>
                                </thead>
                                <tbody id="pricingServiceTableBody">
                                    <tr>
                                        <td>1</td>
                                        <td><input type="text" asp-for="RfqPriceSchedule[1].ItemName" class="form-control" /></td>
                                        <td><textarea rows="3" asp-for="RfqPriceSchedule[1].ItemDescription" class="form-control"></textarea></td>
                                        @*<td><input type="text" asp-for="RfqPriceSchedule[1].InternalRefrenceCode" class="form-control" /></td>*@
                                        <td>
                                            <input type="text" asp-for="RfqPriceSchedule[1].QuantityRequired" class="form-control pricingQty" />
                                            <input type="hidden" asp-for="RfqPriceSchedule[1].CategoryId" value="@(Com.BudgetMetal.Common.Constants_CodeTable.Code_RfqPriceCategory_Service)" class="form-control" />
                                        </td>
                                        <td><button type="button" class="btn btn-danger waves-effect" onclick='RemovePricingRow(this)'>Remove</button></td>
                                    </tr>
                                </tbody>
                            </table>
                            <div class="row">
                                <div class="form-group col-sm-12">
                                    <a class="btn btn-success" id="btnServicePricing"> Add Service Pricing </a>
                                </div>
                            </div>

                            <table id="pricingWarrantyTable" class="table table-striped">
                                <thead>
                                    <tr>
                                        <th width="5%">Serial</th>
                                        <th width="30%">Item Name</th>
                                        <th width="40%">Item Description</th>
                                        @*<th width="20%">Internal Reference Code</th>*@
                                        <th width="15%">Quantity</th>
                                        <th>&nbsp;</th>
                                    </tr>
                                </thead>
                                <tbody id="pricingWarrantyTableBody">
                                    <tr>
                                        <td>1</td>
                                        <td><input type="text" asp-for="RfqPriceSchedule[2].ItemName" class="form-control" /></td>
                                        <td><textarea rows="3" asp-for="RfqPriceSchedule[2].ItemDescription" class="form-control"></textarea></td>
                                        @*<td><input type="text" asp-for="RfqPriceSchedule[2].InternalRefrenceCode" class="form-control" /></td>*@
                                        <td>
                                            <input type="text" asp-for="RfqPriceSchedule[2].QuantityRequired" class="form-control pricingQty" />
                                            <input type="hidden" asp-for="RfqPriceSchedule[2].CategoryId" value="@(Com.BudgetMetal.Common.Constants_CodeTable.Code_RfqPriceCategory_Warranty)" class="form-control" />
                                        </td>
                                        <td><button type="button" class="btn btn-danger waves-effect" onclick='RemovePricingRow(this)'>Remove</button></td>
                                    </tr>
                                </tbody>
                            </table>
                            <div class="row">
                                <div class="form-group col-sm-12">
                                    <a class="btn btn-success" id="btnWarrantyPricing"> Add Warranty Pricing </a>
                                </div>
                            </div>
                        </div>

                        <div role="tabpanel" class="tab-pane fade" id="invited_suppliers">
                            <div class="row">
                                @*<div class="col-sm-3">

                                    <select id="ddlIndustry" class="form-control selectpicker"></select>
                                    <input type="hidden" asp-for="IndustryOfRfq" id="hdIndustry" />
                                </div>*@
                                <div class="col-sm-8">
                                    <select asp-for="SelectedTags" class="form-control selectpicker" id="ddlServiceTags" data-live-search="true" multiple></select>
                                </div>
                                <div class="col-sm-1">

                                </div>
                            </div>
                            <div class="row">
                                <div class="col-sm-4">
                                    <div class="form-group">
                                        <div class="form-line">
                                            <input type="text" name="txtSearchSupplier" id="txtSearchSupplier" class="form-control" placeholder="Supplier Name, UEN">

                                        </div>
                                    </div>
                                </div>
                                <div class="col-sm-3">
                                    <a class="btn btn-success" id="btnLoadSupplier">Search Supplier</a>
                                </div>
                            </div>
                            <table id="invitedSupplierTable" class="table table-striped">
                                <thead>
                                    <tr>
                                        <th width="95%">Supplier Information</th>
                                        <th width="*">&nbsp;</th>
                                    </tr>
                                </thead>
                                <tbody></tbody>
                            </table>
                            <div class="row">
                                <div class="pagination" style="padding:10px;">

                                </div>
                            </div>
                        </div>

                        <div role="tabpanel" class="tab-pane fade" id="attachments">

                            <table id="attachmentTable" class="table table-striped">
                                <thead>
                                    <tr>
                                        <th width="25%">File Name</th>
                                        <th width="70%">Description</th>
                                        <th width="*">&nbsp;</th>
                                    </tr>
                                </thead>
                                <tbody></tbody>
                            </table>
                            <div class="row">
                                <div class="form-group col-sm-12">
                                    <a class="btn btn-success" id="btnNewFile"> New Attachement </a>
                                </div>
                            </div>
                        </div>

                        <div role="tabpanel" class="tab-pane fade" id="settings">
                            <div class="row">
                                <div class="col-sm-5">
                                    <select id="ddlUser" class="form-control selectpicker"></select>
                                </div>
                                <div class="col-sm-5">
                                    @*<select class="form-control show-tick" id="ddlRole" data-live-search="true" multiple></select>*@
                                    <select class="form-control selectpicker" id="ddlRole" data-live-search="true" multiple></select>
                                </div>
                                <div class="col-sm-2">
                                    <a id="btnAddUser" class="btn btn-success">Add Role</a>
                                </div>
                            </div>

                            <table id="userSettingTable" class="table table-hover">
                                <thead>
                                    <tr>
                                        <th width="95%">Accessible User(s)</th>
                                        <th width="*">&nbsp;</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr>
                                        <td>
                                            <input type="hidden" name="documentUserId[]" value="@ViewBag.User_Id" />
                                            <h5> @ViewBag.FullName </h5>
                                            <input type="hidden" name="documentUserRole[]" value="3" />
                                            <span class="label bg-deep-purple">RFQ Creator</span>
                                        </td>
                                        <td></td>
                                    </tr>
                                </tbody>
                            </table>

                            <div id="rfq_Email_Invites">
                                <input type="hidden" id="detailEmalsInviteLastId" value="1" />
                                <table id="reqEmailInvitesTable" class="table table-striped">
                                    <thead>
                                        <tr>
                                            <th width="30%">Name</th>
                                            <th width="50%">Email Address</th>
                                            @*<th width="50%">AccessCode</th>*@
                                            <th>&nbsp;</th>
                                        </tr>
                                    </thead>
                                    <tbody id="rfqEmailInvites">
                                        <tr>
                                            <td><input type="text" asp-for="RfqEmailInvites[0].Name" class="form-control" /></td>
                                            <td><input type="text" asp-for="RfqEmailInvites[0].EmailAddress" class="form-control" /></td>
                                            @*<td><input type="text" asp-for="RfqEmailInvites[0].AccessCode" class="form-control" /></td>*@
                                            <td><button type="button" class="btn btn-danger waves-effect" onclick='RemoveRfqEmailInvites(this)'>Remove</button></td>
                                        </tr>
                                    </tbody>
                                </table>
                                <div class="row">
                                    <div class="form-group col-sm-12">
                                        <a class="btn btn-success" id="btnRfqEmailsInvites"> New Email Invite </a>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
            <div class="card">
                <div class="body">
                    <input type="hidden" name="btnType" id="btnType" value="" />
                    <button type="button" id="btnDraft" class="btn btn-info btn-lg">Save as Draft</button>
                    <button type="button" id="btnSubmit" class="btn btn-primary  btn-lg">Submit</button>
                    <button type="button" onclick='window.location.href = "@Url.Action("Index", "Rfq")";' class="btn btn-default  btn-lg">Cancel</button>
                </div>
            </div>
        </div>
    </div>
</form>

<div class="modal fade" id="publicModal" role="dialog">
    <div class="modal-dialog">
        <!-- Modal content-->
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h4 class="modal-title">Public</h4>
            </div>
            <div class="modal-body">
                By checking Public, your tender will be published in our <b><a href="https://www.facebook.com/Ezy-Tender-982142328651452/" target="_blank">FACEBOOK Page</a></b> and our TenderBoard public domain.
            </div>
        </div>
    </div>
</div>
<div class="modal fade" id="timelineModal" role="dialog">
    <div class="modal-dialog">
        <!-- Modal content-->
        <div class="modal-content">
